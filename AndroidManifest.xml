        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <uses-library android:name="com.google.android.maps" />
        <activity
            android:label="@string/app_name"
            android:name=".umainehabappActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
		<activity android:name=".heliumActivity" android:label="heliumActivity"></activity>
        <activity android:name=".pathing" android:label="pathing"></activity>
        <activity android:name=".map" android:label="map"></activity>
        <activity android:name=".pathingDatabase" android:label="pathingDatabase"></activity>
    	<activity android:name=".habhub" android:label="habhub"
    	    android:theme="@android:style/Theme.NoTitleBar"></activity>
    </application>
</manifest>